{
	"version": 10 ,
	"configurePresets": [
		{
			"name": "vcpkg",
			"binaryDir": "${sourceDir}/build",
			"generator": "MinGW Makefiles",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": "Release",
				"CMAKE_TOOLCHAIN_FILE": "$end{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
				"VCPKG_TARGET_ARCHITECTURE": "x64-windows",
				"VCPKG_CMAKE_SYSTEM_NAME": "Windows",
				"VCPKG_CMAKE_SYSTEM_PROCESSOR": "x86_64"
			}
		}
	]
}