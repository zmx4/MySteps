    "import math\n",
    "from math import sqrt\n",
    "n = 100\n",
    "cnt = 0\n",
    "for i in range(n):\n",
    "    flag = True\n",
    "    for j in range(2,int(sqrt(n))):\n",
    "        if(i%j==0):\n",
    "            flag = False\n",
    "            break\n",
    "    if(flag):\n",
    "        print(i)\n",
    "        cnt+=1\n",
    "print(\"total:\",cnt)"
   ]
  }
